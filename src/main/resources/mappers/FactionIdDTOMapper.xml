<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.5.10//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.PhamKornbluhGroup.mybatismysqlimpl.IFactionId">
    <!--UNTESTED-->
    <resultMap id="FactionIdMap" type="com.PhamKornbluhGroup.DTO.FactionId" autoMapping="false">
        <id property="dbId" column="dbId"/>
        <result property="factionId" column="factionId" javaType="com.PhamKornbluhGroup.DTO.FactionId"/>
    </resultMap>

    <select id="getEntityById" resultMap="FactionIdMap">
        <![CDATA[
        SELECT
            dbId,
            factionId
        FROM FactionEnum
        WHERE dbId = #{dbId}
		]]>
    </select>

    <insert id="saveEntity" useGeneratedKeys="true" keyProperty="dbId">
        <![CDATA[
			INSERT INTO Faction
			    (factionId, factionName)
			VALUES (#{logbookModsId}, #{factionName})
		]]>
    </insert>

    <update id="updateEntity">
        <![CDATA[
			UPDATE Faction
			SET logbookModsId = #{logbookModsId},
			    factionName = #{factionName}
			WHERE dbId = #{dbId}
		]]>
    </update>

    <delete id="removeEntity">
        <![CDATA[
			DELETE FROM Faction
			WHERE dbId = #{dbId}
		]]>
    </delete>
</mapper>